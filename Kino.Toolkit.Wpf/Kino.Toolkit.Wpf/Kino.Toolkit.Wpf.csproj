<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFrameworks>net45;netcoreapp3.0</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <Version>1.2.1</Version>
    <Authors>dino.c</Authors>
    <Product>Kino.Toolkit.Wpf</Product>
    <Description>A set of wpf toolkit.</Description>
    <Copyright>Copyright 2019 dino.c</Copyright>
    <PackageLicenseExpression>https://raw.githubusercontent.com/DinoChan/Kino.Toolkit.Wpf/master/LICENSE</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/DinoChan/Kino.Toolkit.Wpf</PackageProjectUrl>
    <PackageIconUrl>https://github.com/DinoChan/Kino.Toolkit.Wpf/blob/master/Logo.png?raw=true</PackageIconUrl>
    <PackageTags>WPF Control Toolkit Xaml</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Expression.Interaction" Version="3.0.40218" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
</Project>
